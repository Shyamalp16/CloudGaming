{
  "client": {
    "serverUrlBase": "ws://localhost:3002"
  },
  "host": {
    "targetProcessName": "vlc.exe",
    "window": {
      "resizeClientArea": true,
      "targetWidth": 1920,
      "targetHeight": 1080
    },
    "video": {
      "fps": 200,
      "bitrateStart": 45000000,
      "bitrateMin": 30000000,
      "bitrateMax": 60000000,
      "hwFramePoolSize": 6,
      "fullRange": false,
      "preset": "p7",
      "rc": "cbr",
      "bf": 0,
      "rcLookahead": 0,
      "asyncDepth": 8,
      "surfaces": 16
    },
    "capture": {
      "maxQueueDepth": 12,
      "framePoolBuffers": 8,
      "dropWindowMs": 50,
      "dropMinEvents": 1,
      "mmcss": { "enable": true, "priority": 2 },
      "cursor": false,
      "borderRequired": false
    }
  }
}